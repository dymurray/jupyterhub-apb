{
    "kind": "PersistentVolumeClaim",
    "apiVersion": "v1",
    "metadata": {
        "name": "jupyterhub-db",
        "namespace": "{{ namespace }}",
        "labels": {
            "app": "jupyterhub"
        }
    },
    "spec": {
        "accessModes": [
            "ReadWriteOnce"
        ],
        "resources": {
            "requests": {
                "storage": "1Gi"
            }
        }
    }
}
